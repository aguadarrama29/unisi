<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <class dynamic-insert="false" dynamic-update="false"  name="com.universidad.modelo.negocio.CatUsuario"  schema="public" table="cat_usuario" select-before-update="false" lazy="false">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <many-to-one class="com.universidad.modelo.negocio.CatPerfil" name="catPerfil" >
            <column name="id_perfil" not-null="true"/>
        </many-to-one>
        <property generated="never" lazy="false" name="descripcion" optimistic-lock="true" type="string" unique="false">
            <column length="100" name="descripcion" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="contrasenia" optimistic-lock="true" type="string" unique="false">
            <column length="100" name="contrasenia" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="correo" optimistic-lock="true" type="string" unique="false">
            <column length="200" name="correo"/>
        </property>
        <property generated="never" lazy="false" name="telefono" optimistic-lock="true" type="string" unique="false">
            <column length="100" name="telefono"/>
        </property>
        <property generated="never" lazy="false" name="estatus" optimistic-lock="true" type="java.lang.Boolean" unique="false">
            <column name="estatus"/>
        </property>
    </class>
</hibernate-mapping>
